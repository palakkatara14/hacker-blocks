import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int[] arr=new int[n];
		
		for(int i=0;i<n;i++){
			arr[i]=sc.nextInt();
		}
		int i=0;
		int m=sc.nextInt();
		boolean f=false;
		for(i=0;i<n;i++){
			if(arr[i]==m){
			    f=true;
				break;
			}
		}
		if(f==true){
			System.out.println(i);
		}
		else{
            System.out.println("-1");
		}
    }
}

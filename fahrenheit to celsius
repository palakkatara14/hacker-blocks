import java.util.*;
public class Main {
    public static void main(String args[]) {
        // Your Code Here
		Scanner sc=new Scanner(System.in);
		int minimum=sc.nextInt();
		int maximum=sc.nextInt();
		int step=sc.nextInt();
		int f=minimum;
		for(;f<=maximum;f=f+step){
			int c=(f-32)*5/9;
			
			System.out.println(f+" "+c);

		}

    }
}

import java.util.*;

public class Main {
    public static void main(String args[]) {
       Scanner sc= new Scanner(System.in);
        int n=sc.nextInt();
        int se=0,so=0,i=1;
		while(n>0){
            int r=n%10;
            if(i%2==0) se+=r;
            else so+=r;
            i++;
            n=n/10;
        }
        System.out.println(so);
        System.out.println(se);
        
    }
}
